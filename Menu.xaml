<UserControl x:Class="PaymentStation.Menu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PaymentStation"
             mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <local:ComplexButton x:Name="UCProfileSettingsButton" ButtonTag="ProfileSettingsButton" ButtonImageSource="sources/ProfileSettings.JPG" ButtonSize="40, 40"/>
        <local:ComplexButton x:Name="UCSettingsButton" ButtonTag="SettingsButton" ButtonImageSource="sources/Settings.JPG" ButtonSize="40, 40" Margin="10,0,10,0"/>
        <local:ComplexButton x:Name="UCInfoButton" ButtonTag="InfoButton" ButtonImageSource="sources/Info.JPG" ButtonSize="40, 40"/>
    </StackPanel>
</UserControl>
